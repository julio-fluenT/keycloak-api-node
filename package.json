{
  "name": "keycloak-api-node",
  "version": "1.0.6",
  "description": "A Node.js package for Keycloak authentication and admin API integration with secure token encryption",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm run build",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "pnpm run lint"
  },
  "keywords": [
    "keycloak",
    "authentication",
    "authorization",
    "nestjs",
    "typescript",
    "encryption",
    "security"
  ],
  "author": "Julio",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "axios": "^1.6.0",
    "crypto": "^1.0.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
